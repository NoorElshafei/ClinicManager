<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ClinicManager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.ClinicManager.ui.activity.sign_in.SignInActivity"></activity>
        <activity android:name="com.example.ClinicManager.ui.activity.sign_up.SignUpActivity" />
        <activity android:name="com.example.ClinicManager.ui.activity.permission.PermissionScreen" />
        <activity android:name="com.example.ClinicManager.ui.activity.splash.SplashScreen" />
        <activity android:name="com.example.ClinicManager.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>